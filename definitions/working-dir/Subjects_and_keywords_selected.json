// NASA-STI DIVISIONS
{
    "0": {
        "name": "Space Sciences",
        "definition": "Includes space sciences (general); astronomy; astrophysics; lunar and planetary science and exploration; solar physics; and space radiation."
    },
    "1": {
        "name": "Physics",
        "definition": "Includes physics (general); acoustics; atomic and molecular physics; nuclear physics; optics; plasma physics; solid-state physics; and physics of elementary particles and fields."
    },
    "2": {
        "name": "Engineering",
        "definition": "Includes engineering (general); communications and radar; electronics and electrical engineering; fluid mechanics and thermodynamics; instrumentation and photography; lasers and masers; mechanical engineering; quality assurance and reliability; and structural mechanics. For related information see also PHYSICS (categories 70 through 77)."
    }

}


