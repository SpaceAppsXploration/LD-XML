/*
* Example for a Subject
* (JSON doesnt support comments, this is just an example)
 */

{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },

  "@id":"http://mydomain.com/space+sciences",
  "@language": "en",
  "@type": "skos:Concept",

  "skos:prefLabel": { // this is the name of the division
    "@value": "space sciences",
    "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"
  },

  "skos:altLabel": {
    "@value": "space science",
    "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"
  },

  "skos:editorialNote": { // this is how the division is described in NASA-STI
    "@value": "Includes space sciences (general); astronomy; astrophysics; lunar and planetary science and...",
    "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"
  },

  "skos:narrower": [ // this is the list of subjects contained in this division
    {
      "@value": "http://mydomain.com/astronomy#",
      "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
    },
    {
      "@value": "http://mydomain.com/astrophysics#",
      "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
    },
    {
      "@value": "http://mydomain.com/solar+physics#",
      "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
    },
    {
      "@value": "http://mydomain.com/space+radiation#",
      "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
    },
    {
      "@value": "http://mydomain.com/lunar+and+planetary+science+and+exploration#",
      "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
    }
  ],

  "skos:exactMatch": [ // these are definitions in other knowledge base
       {
         "@value": "http://dbpedia.org/page/Outline_of_space_science",
         "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
       },
       {
         "@value": "http://rdf.freebase.com/ns/en.space_science",
         "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#resource"
       }
  ]
}